<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
	xmlns:cq="http://www.day.com/jcr/cq/1.0"
	xmlns:jcr="http://www.jcp.org/jcr/1.0"
	xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
	jcr:primaryType="nt:unstructured" jcr:title="header"
	sling:resourceType="cq/gui/components/authoring/dialog">
	<content jcr:primaryType="nt:unstructured"
		sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
		<items jcr:primaryType="nt:unstructured">
			<column jcr:primaryType="nt:unstructured"
				sling:resourceType="granite/ui/components/coral/foundation/container">
				<items jcr:primaryType="nt:unstructured">
					<logoimage jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
						fieldLabel="Link to" name="./logo" rootPath="/content"
						suffix=".html" />
						<logotitle jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
						fieldLabel="title to" name="./logotitle" rootPath="/content"
						suffix=".html" />
						<homelink jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
						fieldLabel="homelink" name="./homelink" rootPath="/content"
						suffix=".html" />
						<hometext jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						fieldLabel="hometext" name="./hometext" rootPath="/content"
						suffix=".html" />
						<blogslink jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
						fieldLabel="blogslink" name="./blogslink" rootPath="/content"
						suffix=".html" />
						<blogstext jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						fieldLabel="blogstext" name="./blogstext" rootPath="/content"
						suffix=".html" />
				</items>
			</column>
		</items>
	</content>
</jcr:root>